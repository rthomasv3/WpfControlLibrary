<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
   <BitmapImage x:Key="_logoSmallImage" UriSource="/WpfControlLibrary;component/Resources/code-compendium-icon-48.png" />

   <SolidColorBrush x:Key="_titleTextBrush" Color="#1e1e1e" />

   <SolidColorBrush x:Key="_subtitleTextBrush" Color="#3c3c3c" />

   <Style x:Key="_titleTextBlock" TargetType="TextBlock">
      <Setter Property="Foreground" Value="{StaticResource _titleTextBrush}" />
      <Setter Property="FontSize" Value="14" />
      <Setter Property="Margin" Value="0,5,0,0" />
   </Style>

   <Style x:Key="_subtitleTextBlock" TargetType="TextBlock">
      <Setter Property="Foreground" Value="{StaticResource _subtitleTextBrush}" />
      <Setter Property="FontSize" Value="13" />
      <Setter Property="Margin" Value="0,2,0,0" />
   </Style>
</ResourceDictionary>